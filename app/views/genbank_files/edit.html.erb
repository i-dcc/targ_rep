<h1>Editing genbank_file</h1>

<% form_for(@genbank_file) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :escell_clone %><br />
    <%= f.text_area :escell_clone %>
  </p>
  <p>
    <%= f.label :targeting_vector %><br />
    <%= f.text_area :targeting_vector %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @genbank_file %> |
<%= link_to 'Back', genbank_files_path %>