<div class="grid_12">
  <h2>Add a New User</h2>
  
  <% form_for @user, :url => users_path do |f| %>
    <%= f.error_messages %>
    <%= render :partial => "form", :object => f %>
    <div>
      <button type="submit" class="button positive">
        <%= image_tag "led-icons/accept.png", :alt => "" %> Register
      </button>
    </div>
  <% end %>
</div>
